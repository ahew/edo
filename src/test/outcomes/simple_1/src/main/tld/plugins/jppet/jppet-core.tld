<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
  <description><![CDATA["jppet core tag library"]]></description>
  <display-name>"jppet core tag library"</display-name>
  <tlib-version>2.2</tlib-version>
  <short-name>jppet</short-name>
  <uri>/jppet-core</uri>

	<tag>
		<description>Loads and returns the list of the Cats available in the system.</description>
		<name>cats</name>
		<tag-class>org.entando.entando.plugins.jppet.aps.tags.CatListTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>Name used to reference the value pushed into the PageContext.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<tag>
		<description>Loads and returns an Cat given a key.</description>
		<name>cat</name>
		<tag-class>org.entando.entando.plugins.jppet.aps.tags.CatTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>Identifier of the Cat to load</description>
			<name>key</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Name used to reference the value pushed into the PageContext.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

</taglib>